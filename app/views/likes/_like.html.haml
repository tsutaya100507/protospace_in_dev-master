- if prototype.like_user(current_user.id)
  = button_to prototype_likes_path(likes, prototype_id: prototype.id), method: :delete, class: "btn btn-default like-button", id: "heart-button", remote: true do
    = image_tag("icon_red_heart.png")
    = prototype.likes_count
- else
  = button_to prototype_likes_path(prototype), class: "btn btn-default like-button", id: "heart-sum", remote: true do
    = image_tag("icon_heart.png")
    = prototype.likes_count
